program multiplication
declare x;
procedure multi (inout num)
{
    i:=1;
    forcase
        case (i <= 10)
        {
            num := num * i;
            print(num);
        }
        default;                # missing default statement here (default is mandatory) #
    ;
}
#main#
{
    input(x);
    call multi(inout x);
}.
